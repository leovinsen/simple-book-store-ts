{
  "name": "online-book-store-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -V --exec ts-node src/index.ts",
    "migration:create": "db-migrate create -m src/database/migrations",
    "migration:up": "db-migrate up -m src/database/migrations",
    "migration:down": "db-migrate down -m src/database/migrations",
    "test:unit": "mocha --require ts-node/register --recursive 'src/**/*.test.ts' --timeout 20000 --exit",
    "test:e2e": "mocha --require ts-node/register --recursive 'e2e/**/*.test.ts' --timeout 20000 --exit",
    "test:all": "mocha --require ts-node/register --recursive 'src/**/*.test.ts e2e/**/*.test.ts' --timeout 20000 --exit",
    "test": "mocha --require ts-node/register --timeout 5000 --exit --recursive"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "db-migrate": "^0.11.13",
    "db-migrate-sqlite3": "^0.5.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "glob": "^10.2.6",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typedi": "^0.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.4",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "node-ts": "^6.0.1",
    "node-ts-dev": "^1.0.0",
    "nodemon": "^2.0.22",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.3",
    "typescript": "^5.1.3"
  }
}
